#include<stdio.h>
int main()
{
   int a[100],i,num,flag=0;
   printf("enter limit");
   scanf("%d",&n);
   printf("enter n nos");
   for(i=0;i<n;i++)
    scanf("%d",&a[i]);
   printf("enter nos to search");
   scanf("%d",&num);
   for(i=0;i<n;i++)
      {
         if(a[i]==num);
           {
              flag=1;
               break;
            }
       }
   if(flag==1)
   printf("nos is found");
   else
   printf("nos is not found");
}

