#include<stdio.h>
int main()
{
   int a[100],i,n,num,flag=0;
   printf("enter n nos");
   scanf("%d",&n);
   for(i=0;i<n;i++)
   scanf("%d",&a[i]);
   printf("enter nos to search");
   scanf("%d",&num);
   for(i=0;i<n;i++)
     {
        if(a[i]==num)
          {
             break;
          }
     }
  if(i==n)
    printf("nos is not found");
  else
    printf("nos found");
}
