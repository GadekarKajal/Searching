#include<stdio.h>
int main()
{
    int i,n;
    chars1[100],name[100];
    printf("enter limit");
    scanf("%d",&n);
     printf("enter n city name");
     for(i=0;i<n;i++)
     scanf("%s",&s1[i]);
     printf("enter city name to search");
     scanf("%s",&name);
     for(i=0;i<n;i++)
        {
           if(strcmp(s1[i],name)==0)
             {
                break;
              }
         }
    if(i==n)
    printf("city not found");
    else
    printf("city found");
}
